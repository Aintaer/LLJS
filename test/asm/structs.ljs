
struct Point {
  function void Point(double x, double y) {
      this->x = x;
      this->y = y;
  }

  double x, y;
}

function int add1(int x) {
  return x + 1;
}

function double square(double x, double y) {
  return x*x + add1(y*y) + int(4.4);
}

function double main() {
  let Point p1(1.2, 3.4);
  let Point p2(1.2, 3.4);
  let Point p3(1.2, 3.4);
  let Point p4(1.2, 3.4);
  let Point p5(1.2, 5.0);

  return square(p1.x, p5.y);
}
